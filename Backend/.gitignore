# .gitignore for backend and frontend

# Node modules
/node_modules
/frontend/node_modules
/backend/node_modules

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.pid.lock

# Directory for instrumented libs generated by jscoverage/JSCover
lib-cov

# Coverage directory used by tools like istanbul
coverage

# nyc test coverage
.nyc_output

# Grunt intermediate storage (https://gruntjs.com/creating-plugins#storing-task-files)
.grunt

# Bower dependency directory (https://bower.io/)
bower_components

# Compiled source
dist
frontend/dist
backend/dist

# Static Assets
public

# Build directory
build
frontend/build
backend/build

# Misc
.DS_Store
*.pem
*.p12
*.key
*.crt
*.csr

# Env files
.env
.env.*.local

# Vite
.vite
frontend/.vite

# Parcel-bundler cache (https://parceljs.org/)
.cache
frontend/.cache
backend/.cache

# Temporary files
tmp
temp

# Editor directories and files
.vscode/*
.idea
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# eslint cache
.eslintcache

# Jest
jest.config.js
frontend/jest.config.js
backend/jest.config.js

# Tailwind
tailwind.config.js
frontend/tailwind.config.js
backend/tailwind.config.js

# PostCSS
postcss.config.js
frontend/postcss.config.js
backend/postcss.config.js

# ESlint
.eslintrc.js
frontend/.eslintrc.js
backend/.eslintrc.js
